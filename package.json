{
  "name": "fools-snippets",
  "displayName": "fools-snippets",
  "description": "Snippets for Fools",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.0.0"
  },
  "keywords": [
    "dart",
    "typescript",
    "snippets",
    "pseduo-code",
    "polyglot",
    "foolscript"
  ],
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "scala",
        "aliases": [
          "Scala",
          "scala"
        ],
        "extensions": [
          ".scala",
          ".sbt",
          ".sc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/dart.json"
      },
      {
        "language": "swift",
        "path": "./snippets/swift.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascriptreact.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescriptreact.json"
      },
      {
        "language": "rust",
        "path": "./snippets/rust.json"
      },
      {
        "language": "go",
        "path": "./snippets/go.json"
      },
      {
        "language": "kotlin",
        "path": "./snippets/kotlin.json"
      },
      {
        "language": "cpp",
        "path": "./snippets/cpp.json"
      },
      {
        "language": "python",
        "path": "./snippets/python.json"
      },
      {
        "language": "haskell",
        "path": "./snippets/haskell.json"
      },
      {
        "language": "julia",
        "path": "./snippets/julia.json"
      },
      {
        "language": "java",
        "path": "./snippets/java.json"
      },
      {
        "language": "csharp",
        "path": "./snippets/csharp.json"
      },
      {
        "language": "ruby",
        "path": "./snippets/ruby.json"
      },
      {
        "language": "scala",
        "path": "./snippets/scala.json"
      },
      {
        "language": "r",
        "path": "./snippets/r.json"
      },
      {
        "language": "markdown",
        "path": "./snippets/markdown.json"
      },
      {
        "language": "powershell",
        "path": "./snippets/powershell.json"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "fools.saveSnippet",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  }
}
