  "FoolScript Anonymous Function": {
    "prefix": ["foolsFuncAnon", "anonFunc", "anonymous", "closure" ,"lambda", "functionLiteral", "foolsFuncLambda"],
    "body": [
      "{ (${1:parameters}) -> ${2:ReturnType} in",
      "    ${3:// Your code here}",
      "}"
    ],
